{
    "name": "Eth Gasstation",
    "description": "Easily view the current gas prices as seen on ethgasstation.info",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "https://ethgasstation.info/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/icon_16.png",
            "32": "/images/icon_32.png",
            "48": "/images/icon_48.png",
            "128": "/images/icon_128.png"
        }
    },
    "icons": {
        "16": "/images/icon_16.png",
        "32": "/images/icon_32.png",
        "48": "/images/icon_48.png",
        "128": "/images/icon_128.png"
    }
}